pipeline
{
    agent any

    environment 
    {
        name="main" 
    }

    stages
    {
        stage('link')
        {
            steps
            {

                git branch:"main" ,url: "https://github.com/Ganesh-Kalyanshetti/Devops-Class.git"
            }

        }
        stage('echo')
        {
            steps
            {
                echo "Hello "
                echo env.name == "main" ? "Hello main" : "hello new bracnh"
            }
        }

    }
    
}
